#include <iostream>
#include "node.h"

using namespace std;

void setNext(node* Node, node* nextNode);

int main(){
  student* S = new student;
  node* N = new node(S);
  cout << N->getStudent()->getValue() << endl;
  node* N1 = new node(S);
  setNext(N, N1);
  cout << N->getNextNode()->getStudent()->getValue() << endl;
}

void setNext(node* Node, node* nextNode){
  Node->setNextNode(nextNode);
}
